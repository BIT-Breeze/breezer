<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tourMain">

	<select id="getTour" parameterType="map" resultType="postvo">
		<![CDATA[
		select *
		  from TB_POST 
		 where userId = #{userId} 
		   and tourIdx = #{tourIdx} 
	  order by tripDateTime asc
		]]>
	</select>

</mapper>


















